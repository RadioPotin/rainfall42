BEGIN main(int argc, char**argv)

  FILE *stream;
  char buffer[]
  char buff[]
  int index;

  stream = fopen("/home/user/end/.pass", "r");
  bzero(buffer, 33);

  if (stream && argc == 2) {
    fread(buffer, 1, 66, stream);
    index = atoi(argv[1]);
    buffer[index] = 0;
    fread(buff, 1, 65, stream);
    fclose(stream);
    if (!strcmp(buff, argv[1])) {
      execl("/bin/sh", "sh", 0)
    } else {
      puts(buff);
    }
    return 0;

  }
  return -1;
END main
